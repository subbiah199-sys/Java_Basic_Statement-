import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int num = scan.nextInt();
        scan.close();
        if (num <= 99) {
            System.out.println("Invalid Input");
            return;
        }
       int sum = 0;
        while (num > 0) {
            sum += num % 10;  
            num /= 10;        
        }
     System.out.println("Sum of digit is " + sum);
    }
}
