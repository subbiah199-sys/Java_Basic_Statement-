import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int num = scanner.nextInt();
        System.out.println("Bird said:" + num);
        int tens = num / 10;
        int ones = num % 10;
        int sum = tens + ones;
        System.out.println("Alice must go in path-" + sum);
    }
}
